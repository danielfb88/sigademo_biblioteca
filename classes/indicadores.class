class BusinessBibliotecaIndicadores extends Business
{
	public $idindicador;
	public $idetiqueta;
	public $idobra;
	public $indicador;
	public $conteudo;
	public $ocorrencia;
	public $obra_temp;
	public $etiqueta_temp;

	function BusinessBibliotecaIndicadores($data = null)
	{
		$this->Business('sigaept', $data);
	}

	public function insertIndicador($idindicador, $idetiqueta, $idobra, $indicador, $conteudo, $ocorrencia) {
		$sql = new sql('idindicador, idetiqueta, idobra, indicador, conteudo, ocorrencia', 'bt_indicadores');

		$values = array(
				$idindicador,
				$idetiqueta,
				$idobra,
				$indicador,
				$conteudo,
				$ocorrencia
			);

		return $this->Execute($sql->Insert($values));
	}

	public function getNextId() {
		$id = $this->_db->GetNewId('seq_bt_indicadores');
		return $id;
	}
}
