class frmMunicipioFind extends MForm {

	function __construct() {
		parent::__construct('Relação de Municípios');
		$this->defaultButton = false;
		$this->eventHandler();
		//$this->createGrid();
	}

	function CreateFields()
	{
		global $MIOLO, $action, $module, $item, $page;

		$this->grid = $this->manager->getUI()->getGrid($module,'gridMunicipio',null);
		$this->grid->HeaderLink('new', 'Novo Registro', $MIOLO->GetActionURL($module,'main:admin:municipioBiblioteca'));

		$fields = array(
			$this->grid
		);
		$this->setFields($fields);
	}

}
