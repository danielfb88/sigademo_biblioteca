	<parameter name="NUMERODOTOMBO" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT pessoa.nome, validacao.codigoacesso, obra.titulo, obra.autor,
exemplar.volume, exemplar.numerodotombo, exemplar.numinventario,
emprestimo.datahoradoemprestimo, emprestimo.datahoraprevisaodevolucao,
emprestimo.datahoradadevolucao

FROM bt_emprestimo emprestimo

INNER JOIN bt_exemplar exemplar ON exemplar.idexemplar = emprestimo.idexemplar
INNER JOIN cm_usuario usuario ON emprestimo.idusuario = usuario.idusuario
INNER JOIN cm_pessoa pessoa ON usuario.idpessoa = pessoa.idpessoa
INNER JOIN bt_obra obra ON exemplar.idobra = obra.idobra
INNER JOIN bt_validacao validacao  ON usuario.idusuario = validacao.idusuario

WHERE
(
exemplar.numerodotombo = $P{NUMERODOTOMBO} AND
to_date(to_char(emprestimo.datahoradadevolucao, 'DD Mon YYYY'), 'DD Mon YYYY') = current_date AND
emprestimo.datahoradadevolucao =
(
SELECT max(emprestimo_subquery1.datahoradadevolucao)
FROM bt_emprestimo emprestimo_subquery1
INNER JOIN bt_exemplar exemplar_subquery1 ON exemplar_subquery1.idexemplar = emprestimo_subquery1.idexemplar
INNER JOIN cm_usuario usuario_subquery1 ON emprestimo_subquery1.idusuario = usuario_subquery1.idusuario
INNER JOIN bt_validacao validacao_subquery1 ON usuario_subquery1.idusuario = validacao_subquery1.idusuario
WHERE
(exemplar_subquery1.numerodotombo =  exemplar.numerodotombo AND
to_date(to_char(emprestimo_subquery1.datahoradadevolucao, 'DD Mon YYYY'), 'DD Mon YYYY') = current_date )))]]>
	</queryString>
